{
  "name": "libdragon",
  "version": "2.0.3",
  "description": "This is a docker wrapper for libdragon",
  "main": "index.js",
  "engines": {
    "node": ">=7.6"
  },
  "directories": {
    "example": "examples"
  },
  "bin": {
    "libdragon": "./index.js"
  },
  "scripts": {
    "start": "node index.js --mount-path=./libdragon-source start",
    "stop": "node index.js stop",
    "make": "node index.js make",
    "download": "node index.js --mount-path=./libdragon-source download",
    "init": "node index.js --mount-path=./libdragon-source init",
    "buildDragon": "node index.js --mount-path=./libdragon-source buildDragon",
    "prepublishOnly": "node index.js --mount-path=./libdragon-source update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anacierdem/libdragon-docker.git"
  },
  "author": "Ali Naci Erdem",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anacierdem/libdragon-docker/issues"
  },
  "homepage": "https://github.com/anacierdem/libdragon-docker#readme",
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
